{
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.rulers": [ 120 ],
  "eslint.enable": true,
  "cSpell.words": [
    "Haier"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "lerna.json"
      ],
      "url": "../node_modules/lerna/schemas/lerna-schema.json"
    }
  ],
  "i18n-ally.localesPaths": [
    "packages/ui-renderer/src/lang"
  ],
}
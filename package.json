{
  "name": "homebridge-plugin-haier",
  "displayName": "Haier",
  "version": "2.0.0",
  "description": "海尔智家 Homebridge 插件",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./homebridge-ui/*": "./homebridge-ui/*",
    "./shared": {
      "import": "./dist/shared.js",
      "require": "./dist/shared.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "build": "rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "check": "biome check --write",
    "dev": "concurrently \"npm run build:watch\" \"npm run watch\"",
    "prewatch": "npm run build && npm link",
    "watch": "nodemon",
    "format": "biome format --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baranwang/homebridge-plugin-haier.git"
  },
  "bugs": {
    "url": "https://github.com/baranwang/homebridge-plugin-haier/issues"
  },
  "homepage": "https://github.com/baranwang/homebridge-plugin-haier",
  "engines": {
    "node": "^18.20.4 || ^20.18.0 || ^22.10.0",
    "homebridge": "^1.8.0 || ^2.0.0-beta.0"
  },
  "files": [
    "dist",
    "homebridge-ui",
    "config.schema.json",
    "CHANGELOG.md"
  ],
  "keywords": [
    "homebridge-plugin",
    "haier",
    "海尔"
  ],
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^2.0.0",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rslib/core": "^0.2.2",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "ahooks": "^3.8.4",
    "concurrently": "^9.1.2",
    "date-fns": "^4.1.0",
    "homebridge": "^1.8.5",
    "homebridge-config-ui-x": "^4.67.0",
    "nodemon": "^3.1.9",
    "rc-field-form": "^2.7.0",
    "react": "^19.0.0",
    "react-bootstrap": "^1.6.8",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "ahooks": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
